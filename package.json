{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail -n SERVER,CLIENT -c green,cyan \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "cd client && vite",
    "build": "npm run build:server",
    "build:client": "cd client && vite build --emptyOutDir",
    "build:server": "cd server && tsc --project tsconfig.json --outDir ../dist-server",
    "start": "NODE_ENV=production node dist-server/server/production-server.js",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "deploy": "npm run build && gh-pages -d dist/client",
    "deploy:vercel": "cd client && npm install && vite build --emptyOutDir",
    "vercel-build": "cd client && vite build --emptyOutDir",
    "render-build": "npm install && npm run build:server",
    "render-start": "NODE_ENV=production node dist-server/production-server.js"
  },
