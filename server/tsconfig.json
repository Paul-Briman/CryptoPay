{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist-server",
    "module": "CommonJS",
    "target": "ES2020",
    "moduleResolution": "node",
    "allowImportingTsExtensions": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "../client",
    "../dist-server"
  ]
}