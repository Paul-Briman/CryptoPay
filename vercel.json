{
  "version": 2,
  "builds": [
    {
      "src": "package.json",  
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist",
         "buildCommand": "npm run vercel-build"  
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "client/dist/$1"
    }
  ]
}